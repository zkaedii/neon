{
  "dashboard": {
    "title": "Neon Tokyo - System Dashboard",
    "tags": ["neon-tokyo", "production"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Video Generation Requests",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(video_generation_requests_total[5m])",
            "legendFormat": "Requests/sec"
          }
        ]
      },
      {
        "id": 2,
        "title": "Active Jobs",
        "type": "stat",
        "targets": [
          {
            "expr": "video_generation_active_jobs",
            "legendFormat": "Active"
          }
        ]
      },
      {
        "id": 3,
        "title": "Queue Size",
        "type": "graph",
        "targets": [
          {
            "expr": "video_generation_queue_size",
            "legendFormat": "Queue Size"
          }
        ]
      },
      {
        "id": 4,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(video_generation_errors_total[5m])",
            "legendFormat": "Errors/sec"
          }
        ]
      },
      {
        "id": 5,
        "title": "GPU Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "gpu_memory_used_bytes / gpu_memory_total_bytes * 100",
            "legendFormat": "GPU Memory %"
          }
        ]
      }
    ],
    "refresh": "10s",
    "schemaVersion": 27,
    "version": 1
  }
}
